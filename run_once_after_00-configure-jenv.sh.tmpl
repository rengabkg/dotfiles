#!/usr/bin/env zsh

set -euo pipefail

eval "$(jenv init -)"
jenv enable-plugin export
jenv enable-plugin gradle

{{ with $computerName := output "scutil" "--get" "ComputerName" | trim }}
{{ if eq $computerName "Athena" }}
jenv global 1.8
jenv shell 1.8
{{ else if eq $computerName "Tyche" }}
jenv global 11.0
jenv shell 11.0
{{ end }}
{{ end }}
